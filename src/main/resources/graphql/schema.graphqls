type Mutation{
    createBook( book:BookInput):Book

}

type Query{

     allBooks:[Book]
     getBook(bookId:Int):Book
    }

type Book{

    id:ID!
    title:String
    description:String
    author:String
    price:Float
    noOfPages:Int

}

input  BookInput {
    title:String
    description:String
    author:String
    price:Float
    noOfPages:Int

}